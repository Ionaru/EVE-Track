    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "angular2-fontawesome": "^0.8.0",
    "angular2-logger": "^0.5.1",
    "core-js": "^2.4.1",
    "crypto-js": "^3.1.9-1",
    "font-awesome": "^4.7.0",
    "rxjs": "^5.1.0",
    "socket.io": "^1.7.2",
    "socket.io-client": "^1.7.2",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
