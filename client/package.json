{
  "name": "eve-track-client",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "ng serve --proxy-config proxy.conf.ts --no-progress --extract-css=true --sourcemap",
    "build": "ng build --aot=true --extract-css --progress --sourcemap --prod --verbose",
    "lint": "tslint --project tsconfig.json --type-check -c tslint.json \"src/**/*.ts\"",
    "slint": "stylelint --config stylelintrc.json \"src/**/*.scss\"",
    "test": "ng test --single-run --code-coverage",
    "watchtest": "ng test",
    "saucelabs": "export SAUCELABS=true && npm run test && unset SAUCELABS",
    "browserstack": "export BROWSERSTACK=true && npm run test && unset BROWSERSTACK",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "autotest": "npm run lint && npm run slint && npm run test && npm run saucelabs && npm run browserstack && npm run build"
  },
  "dependencies": {
    "@angular/animations": "^4.2.5",
    "@angular/common": "^4.2.5",
    "@angular/compiler": "^4.2.5",
    "@angular/core": "^4.2.5",
    "@angular/forms": "^4.2.5",
    "@angular/http": "^4.2.5",
    "@angular/platform-browser": "^4.2.5",
    "@angular/platform-browser-dynamic": "^4.2.5",
    "@angular/router": "^4.2.5",
    "angular2-logger": "^0.6.0",
    "bootstrap": "next",
    "bootswatch": "next",
    "core-js": "^2.4.1",
    "countdown": "^2.6.0",
    "crypto-js": "^3.1.9-1",
    "font-awesome": "^4.7.0",
    "ngx-bootstrap": "^1.7.1",
    "rxjs": "^5.4.1",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3",
    "ts-helpers": "^1.1.1",
    "xml2js": "^0.4.17",
    "zone.js": "^0.8.12"
  },
  "devDependencies": {
    "@angular/cli": "^1.2.0",
    "@angular/compiler-cli": "^4.2.5",
    "@types/countdown": "^0.0.5",
    "@angular/language-service": "^4.2.5",
    "@types/chai": "^4.0.1",
    "@types/crypto-js": "^3.1.33",
    "@types/karma": "^0.13.36",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.6",
    "@types/sinon": "^2.3.2",
    "@types/socket.io": "^1.4.28",
    "@types/socket.io-client": "^1.4.29",
    "@types/xml2js": "^0.4.0",
    "codelyzer": "^3.1.1",
    "karma": "^1.7.0",
    "karma-browserstack-launcher": "^1.3.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sauce-launcher": "^1.1.0",
    "mocha": "^3.4.2",
    "must": "^0.13.4",
    "node-sass": "^4.5.3",
    "protractor": "^5.1.2",
    "sinon": "^2.3.6",
    "stylelint": "^7.12.0",
    "stylelint-config-sass-guidelines": "^2.1.0",
    "stylelint-config-standard": "^16.0.0",
