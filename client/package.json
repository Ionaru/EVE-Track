    "@types/crypto-js": "^3.1.33",
    "@types/karma": "^0.13.33",
    "@types/mocha": "^2.2.39",
    "@types/node": "^7.0.5",
    "@types/sinon": "^1.16.35",
    "@types/socket.io": "^1.4.28",
    "@types/socket.io-client": "^1.4.29",
    "chai": "^3.5.0",
    "codelyzer": "^2.0.1",
    "karma": "^1.5.0",
    "karma-browserstack-launcher": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
