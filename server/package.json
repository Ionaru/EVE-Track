{
    "name": "evie-server",
    "version": "0.7.13",
    "author": "Jeroen Akkerman",
    "description": "Server module for EVIE",
    "repository": {
        "type": "git",
        "url": "https://github.com/Ionaru/EVIE.git"
    },
    "scripts": {
        "typeorm": "node --require ts-node/register node_modules/typeorm/cli.js",
        "migrate": "npm run typeorm migration:run",
        "clean": "npx -q rimraf dist",
        "build": "npm run clean && tsc --project tsconfig.json",
        "lint": "npm run lint:src && npm run lint:test",
        "lint:src": "tslint --project tsconfig.json -c tslint.json --format stylish",
        "lint:test": "tslint -c tslint.json src/**/*.spec.ts --format stylish",
        "pretest": "npm run lint",
        "test": "echo \"Error: no test specified\" && exit 0",
        "posttest": "tsc --project tsconfig.json --noEmit",
        "start": "npm run migrate && node ./dist/src/index.js",
        "preversion": "npm run test"
    },
    "dependencies": {
        "@ionaru/esi-service": "^3.2.2",
        "@ionaru/eve-utils": "^4.4.0",
        "@ionaru/random-string": "^1.1.1",
        "@ionaru/web-server": "^3.0.0",
        "@sentry/node": "^5.9.0",
        "agentkeepalive": "^4.1.0",
        "app-root-path": "^3.0.0",
        "axios": "^0.19.0",
        "body-parser": "^1.19.0",
        "chalk": "^3.0.0",
        "clone": "^2.1.2",
        "compression": "^1.7.3",
        "cors": "^2.8.5",
        "debug": "^4.1.1",
        "express": "^4.17.1",
        "express-mysql-session": "^2.0.1",
        "express-session": "^1.17.0",
        "http-status-codes": "^1.4.0",
        "jsonwebtoken": "^8.3.0",
        "mysql": "^2.17.1",
        "on-finished": "^2.3.0",
        "reflect-metadata": "^0.1.13",
        "socket.io": "^2.3.0",
        "socket.io-express-session": "^0.1.3",
        "source-map-support": "^0.5.16",
        "typeorm": "^0.2.20"
    },
    "devDependencies": {
        "@types/app-root-path": "^1.2.4",
        "@types/bcryptjs": "^2.4.2",
        "@types/body-parser": "^1.17.1",
        "@types/clone": "^0.1.30",
        "@types/compression": "^1.0.1",
        "@types/cors": "^2.8.6",
        "@types/debug": "^4.1.5",
        "@types/express": "^4.17.2",
        "@types/express-mysql-session": "^1.2.2",
        "@types/express-serve-static-core": "^4.17.0",
        "@types/express-session": "^1.15.16",
        "@types/jsonwebtoken": "^8.3.5",
        "@types/mysql": "^2.15.8",
        "@types/node": "^12.12.12",
        "@types/on-finished": "^2.3.1",
        "@types/socket.io": "^2.1.4",
        "@types/source-map-support": "^0.5.0",
        "ts-node": "^8.5.2",
        "tslint": "^5.20.1",
        "tslint-sonarts": "^1.9.0",
        "typescript": "^3.7.2"
    },
    "license": "MIT"
}
