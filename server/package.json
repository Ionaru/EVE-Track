{
  "name": "eve-track-server",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "autotest": "npm run lint && npm run test-cov && npm run report",
    "start": "node ./src/bin/start-server",
    "lint": "tslint --type-check -c tslint.json \"src/**/*.ts\"",
    "test": "_mocha --compilers ts:ts-node/register ./src/test",
    "test-cov": "nyc yarn test && yarn report",
    "report": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.1",
    "concurrently": "^3.4.0",
    "express": "^4.15.2",
    "express-mysql-session": "^1.2.0",
    "express-serve-static-core": "^0.1.1",
    "express-session": "^1.15.2",
    "express-socket.io-session": "^1.3.2",
    "helmet": "^3.5.0",
    "ini": "^1.3.4",
    "jsesc": "^2.4.0",
    "mkdirp": "^0.5.1",
    "mysql": "^2.13.0",
    "node-fetch": "^1.6.3",
    "path": "^0.12.7",
    "sequelize": "^3.30.4",
    "socket.io": "^1.7.3",
    "socket.io-express-session": "^0.1.3",
    "ts-node": "^3.0.2",
    "typescript": "^2.2.2",
    "winston": "^2.3.1",
    "winston-daily-rotate-file": "^1.4.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.0",
    "@types/body-parser": "^1.16.3",
    "@types/chai": "^3.4.35",
    "@types/express": "^4.0.35",
    "@types/express-serve-static-core": "^4.0.44",
    "@types/express-session": "^0.0.32",
    "@types/helmet": "^0.0.34",
    "@types/ini": "^1.3.29",
    "@types/jsesc": "^0.4.29",
    "@types/mkdirp": "^0.3.29",
    "@types/mocha": "^2.2.40",
    "@types/mysql": "0.0.31",
    "@types/node": "^7.0.12",
    "@types/node-fetch": "^1.6.7",
    "@types/sequelize": "^4.0.49",
    "@types/sinon": "^2.1.2",
    "@types/socket.io": "^1.4.29",
    "@types/winston": "^2.3.0",
    "chai": "^3.5.0",
    "coveralls": "^2.13.0",
    "mocha": "^3.2.0",
    "nyc": "^10.2.0",
    "sinon": "^2.1.0",
    "tslint": "^4.5.1"
  },
  "license": "MIT"
}
