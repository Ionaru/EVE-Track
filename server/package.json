        "body-parser": "^1.18.2",
        "chalk": "^2.4.1",
        "compression": "^1.7.3",
        "cookie-parser": "^1.4.3",
        "cors": "^2.8.4",
        "express": "^4.16.3",
        "express-mysql-session": "^2.0.0",
        "express-serve-static-core": "^0.1.1",
        "express-session": "^1.15.6",
        "fs-extra": "^7.0.0",
        "helmet": "^3.12.1",
        "http-status-codes": "^1.3.0",
        "ini": "^1.3.5",
        "mysql": "^2.16.0",
        "node-fetch": "^2.1.2",
        "on-finished": "^2.3.0",
        "reflect-metadata": "^0.1.12",
        "socket.io": "^2.1.1",
        "socket.io-express-session": "^0.1.3",
